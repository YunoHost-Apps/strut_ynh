{
    "name": "Strut",
    "id": "strut",
    "packaging_format": 1,
    "description": {
        "en": "Slide editor for creating impress.js presentations",
        "fr": "Éditeur de diapositives permettant de créer des présentations impress.js"
    },
    "version": "20220404~ynh1",
    "url": "http://strut.io",
    "upstream": {
        "license": "AGPL-3.0",
        "website": "http://strut.io",
        "demo": "http://strut.io/dist/",
        "code": "https://github.com/MohannadNaj/Strut"
    },
    "license": "AGPL-3.0",
    "maintainer": {
        "name": "ljf",
        "email": "ljf+yunohost@grimaud.me"
    },
    "requirements": {
        "yunohost": ">= 11.0.9"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/strut",
                "default": "/strut"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
