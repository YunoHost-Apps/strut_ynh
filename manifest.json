{
    "name": "Strut",
    "id": "strut",
    "packaging_format": 1,
    "version": "20171224-1~ynh2",
    "url": "http://strut.io",
    "license": "AGPL-3.0",
    "requirements": {
        "yunohost": ">= 3.8.1"
    },
    "description": {
        "en": "Slide editor for creating impress.js presentations.",
        "fr": "Éditeur de diapositives permettant de créer des présentations impress.js."
    },
    "maintainer": {
        "name": "ljf",
        "email": "ljf+yunohost@grimaud.me"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "ask": {
                    "en": "Choose a domain for Strut",
                    "fr": "Choisissez un nom de domaine pour Strut"
                },
                "example": "example.com"
            },
            {
                "name": "path",
                "type": "path",
                "ask": {
                    "en": "Choose a path for Strut",
                    "fr": "Choisissez l'adresse pour Strut"
                },
                "example": "/strut",
                "default": "/strut"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "ask": {
                    "en": "Could non member create presentations?",
                    "fr": "Une personne non membre peut elle créer une présentation ?"
                },
                "default": true
            }


        ]
    }
}
